            <kind>1</kind>
            <string>///Start moving

//Stop him
alarm[2] = 120;

//Set the horizontal speed
if (!boo) { //If the horizontal speed has not been set.

    //Move
    boo = true;
    
    //If Mario does exist.
    if (instance_exists(obj_mario)) {
    
        //If Mario is at the right.
        if (obj_mario.x &gt; x) {
        
            hspeed = 1;
        }
        
        //Otherwise, if it's at the left.
        else if (obj_mario.x &lt; x) {
        
            hspeed = -1;
        }
    }
    
    //Otherwise, if it does not exist.
    else {
    
        //If the level manager is at the right.
        if (obj_levelmanager.x &gt; x) {
        
            hspeed = 1;
        }
        
        //Otherwise, if it's at the left.
        else if (obj_levelmanager.x &lt; x) {
        
            hspeed = -1;
        }    
    }
}

//Otherwise, if the horizontal speed was set already, use the previous horizontal speed.
else {

    hspeed = prevhspeed;
}
</string>
