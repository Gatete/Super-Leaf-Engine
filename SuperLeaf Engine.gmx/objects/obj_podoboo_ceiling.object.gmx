            <kind>1</kind>
            <string>///Podoboo logic

//Make it fall when reaching start position.
if ((y &gt; ystart-39) &amp;&amp; (ready == true)) {

    //Make it fall.
    ready = false;
    gravity = 0.2;
    gravity_direction = 90;
}

//Bounce if outside the room.
if (bbox_top &lt; -16) {

    if (!ready) {
    
        //Make it stop
        ready = true;
        
        //Stop after 10 steps.
        alarm[0] = 10;
    }
}

//Bounce in contact with lava.
lava = collision_rectangle(bbox_left,bbox_top,bbox_right,bbox_top,obj_lava_ceiling,0,0);
if (lava) {

    //Bounce.
    if (!ready) &amp;&amp; (vspeed &lt; 0) {

        //Make it stop
        ready = true;
        
        //Stop after 10 steps.
        alarm[0] = 10;
        
        //Create lava splash
        ID = instance_create(x,lava.bbox_bottom-4,obj_smoke);
        with (ID) {
        
            sprite_index = spr_splash_lava_b;
        }
    }
}

//Set the scale.
xscale = 1;

