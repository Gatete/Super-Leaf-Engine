          <argument>
            <kind>1</kind>
            <string>///Transform into Frog Mario

//Deny if Mario is behind the scenery.
if (obj_mario.depth == 16) {

    if (obj_mario.tile12 &gt; -1) || (obj_mario.tile15 &gt; -1)
        exit;
}

//Get 1000 points
with (instance_create(x-8,y,obj_score)) event_user(4);

//If Mario does not have the frog powerup.
if (global.powerup != cs_frog) {

    //Play 'Powerup' sound.
    audio_play_sound(snd_transform,0,0);
    
    //Set up 'Raccoon' powerup.
    global.powerup = cs_frog;
    
    //Initialize animation sequence.
    with (instance_create(0,0,obj_mario_transform)) sequence = 3;
}

//Otherwise, if Mario does have the frog powerup.
else {

    //Play 'Powerup' sound.
    audio_play_sound(snd_powerup,0,0);
}

//Destroy
instance_destroy();
</string>
