            <kind>1</kind>
            <string>///Bullet Bill logic

//If the missile bill can't detect Mario.
if (homing == 0) {

    //Facing direction
    xscale = 1*sign(hspeed);
}

//Otherwise, if the bullet bill can detect Mario, turn back at him.
else if (homing &gt; 0) {

    //Find Mario
    if (homing == 1) {
    
        //If Mario does exist.
        if (instance_exists(obj_mario)) {
        
            //If Mario is at the left and the 
            if ((obj_mario.x &lt; x) &amp;&amp; (xscale == 1)) {
            
                //Set the facing direction
                xscale = -1;
                
                //Follow Mario
                homing = 2;
            }
            
            //Otherwise
            else if ((obj_mario.x &gt; x) &amp;&amp; (xscale == -1)) {
            
                //Set the facing direction.
                xscale = 1;
                
                //Follow Mario
                homing = 2;
            }
        }
    }
    
    //Turn at Mario.
    else if (homing == 2) {
    
        //Increment / Decrement speed.
        if (xscale == 1)
            hspeed += 0.05;
        else if (xscale == -1)
            hspeed -= 0.05;
    }
    
    //Cap horizontal speed
    if (abs(hspeed) &gt; 2)
        hspeed = 2*sign(hspeed);
}

//Change depth
if ((depth == 10) &amp;&amp; (!place_meeting(x,y,obj_solid)))
    depth = -2;
</string>
